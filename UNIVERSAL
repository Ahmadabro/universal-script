function dellEsp()
    for i, player in pairs(workspace:GetDescendants()) do
        if player.Name == "BillobardEsp" and player:IsA("BillboardGui") then
            player:Destroy()
        end
        if player.Name == "EspBox" then
            player:Destroy()
        end
    end
end
	
   end,
})

