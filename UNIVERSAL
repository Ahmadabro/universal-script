            spawn(function()
                while player.Parent do
                    local distance = getDistance(localPlayer.Position, player.Position)
                    TextLabel.Text = player.Parent.Name .. " (" .. math.floor(distance) .. ")"
                    wait() 
                end
            end)
        end
    end
end
 
